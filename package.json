{
  "name": "vscode-astral-gem-theme",
  "version": "1.9.3",
  "icon": "assets/logo.png",
  "displayName": "The Astral Themes Collection",
  "description": "A collection of color schemas, inspired by the Google Gemini code snippets hightlighting and the Dark+ theme of VSCode.",
  "license": "MIT",
  "author": "Simón Villafañe",
  "publisher": "itssimmons",
  "homepage": "https://github.com/itssimmons/astral-gem-theme",
  "repository": {
    "type": "git",
    "url": "https://github.com/itssimmons/astral-gem-theme"
  },
  "keywords": [
		"dark plus",           "dark+",             "vsode default theme",
		"improved theme",      "dark theme",        "astroid",
		"asteroid",            "gemini",            "astral",
		"vscode",              "theme",             "color theme",
		"color-theme",         "colors",            "syntax",
		"highlighting",        "highlight",         "syntax highlighting",
		"syntax highlight",    "code highlighting", "code highlight"
  ],
  "categories": [
    "Themes"
  ],
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=9.0.0",
    "vscode": "^1.67.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "(Legacy) Astroid Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/legacy-astroid-theme.color-theme.json"
      },
      {
        "label": "Astral Gem",
        "uiTheme": "vs-dark",
        "path": "./themes/astral-gem.color-theme.json"
      }
    ]
  },
  "scripts": {
    "publish": "npx vsce publish",
    "package": "npx vsce package",
    "build": "tsx src/build.mts",
    "build:watch": "tsx watch src/build.mts",
    "test": "node --test src/**/*.spec.ts",
    "code:fix": "pnpm format && pnpm lint",
    "format": "npx prettier -w src",
    "lint": "npx eslint --config eslint.config.mjs"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.3",
    "@vscode/vsce": "^3.5.0",
    "chalk": "^4.1.2",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "tsx": "^4.20.3",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.34.1",
    "yaml": "^2.3.1"
  }
}
